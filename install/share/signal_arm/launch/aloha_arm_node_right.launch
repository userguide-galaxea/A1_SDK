<launch>
    <arg name="host_serial_port_path"         default="/dev/test_dvt1_12"/>
    <arg name="host_joint_states_topic"       default="/joint_states_host_right"/>
    <arg name="host_gripper_feedback_topic"   default="/gripper_stroke_host_right"/>
    <arg name="host_control_topic"            default="/arm_joint_command_host_right"/>
    <arg name="host_arm_status_topic"         default="/arm_status_host_right"/>
    <arg name="host_gripper_topic"            default="/gripper_aloha_command_host_right"/>
    <arg name="host_joint_names"              default="arm_joint1,arm_joint2,arm_joint3,arm_joint4,arm_joint5,arm_joint6,gripper"/>
    <arg name="host_gripper_force_control_topic" default="/gripper_force_control_host_right"/>
    <arg name="host_gripper_position_control_topic" default="/gripper_position_control_host_right"/>
    <arg name="host_gripper_version" default="G2"/>
    <!-- node starter-->
    <node pkg="signal_arm" type="iarm_node" name="iarm_node_host_right" output="screen">
        <param name="serial_port_path"              value="$(arg host_serial_port_path)"/>
        <param name="joint_states_topic"            value="$(arg host_joint_states_topic)"/>
        <param name="gripper_feedback_topic"        value="$(arg host_gripper_feedback_topic)"/>
        <param name="control_topic"                 value="$(arg host_control_topic)"/>
        <param name="arm_status_topic"              value="$(arg host_arm_status_topic)"/>
        <param name="gripper_topic"                 value="$(arg host_gripper_topic)"/>
        <param name="joint_names"                   value="$(arg host_joint_names)"/>
        <param name="gripper_force_control_topic"   value="$(arg host_gripper_force_control_topic)"/>
        <param name="gripper_position_control_topic" value="$(arg host_gripper_position_control_topic)"/>
        <param name="gripper_version"               value="$(arg host_gripper_version)"/>
    </node>


    <arg name="slave_serial_port_path"         default="/dev/test_dvt1_17"/>
    <arg name="slave_joint_states_topic"       default="/joint_states_slave_right"/>
    <arg name="slave_gripper_feedback_topic"   default="/gripper_stroke_slave_right"/>
    <arg name="slave_control_topic"            default="/arm_joint_command_slave_right"/>
    <arg name="slave_arm_status_topic"         default="/arm_status_slave_right"/>
    <arg name="slave_gripper_topic"            default="/gripper_aloha_command_slave_right"/>
    <arg name="slave_joint_names"              default="arm_joint1,arm_joint2,arm_joint3,arm_joint4,arm_joint5,arm_joint6,gripper"/>
    <arg name="slave_gripper_force_control_topic" default="/gripper_force_control_slave_right"/>
    <arg name="slave_gripper_position_control_topic" default="/gripper_position_control_slave_right"/>
    <arg name="slave_gripper_version" default="G2"/>
    <!-- node starter-->
    <node pkg="signal_arm" type="iarm_node" name="iarm_node_slave_right" output="screen">
        <param name="serial_port_path"              value="$(arg slave_serial_port_path)"/>
        <param name="joint_states_topic"            value="$(arg slave_joint_states_topic)"/>
        <param name="gripper_feedback_topic"        value="$(arg slave_gripper_feedback_topic)"/>
        <param name="control_topic"                 value="$(arg slave_control_topic)"/>
        <param name="arm_status_topic"              value="$(arg slave_arm_status_topic)"/>
        <param name="gripper_topic"                 value="$(arg slave_gripper_topic)"/>
        <param name="joint_names"                   value="$(arg slave_joint_names)"/>
        <param name="gripper_force_control_topic"   value="$(arg slave_gripper_force_control_topic)"/>
        <param name="gripper_position_control_topic" value="$(arg slave_gripper_position_control_topic)"/>
        <param name="gripper_version"               value="$(arg slave_gripper_version)"/>
    </node>
</launch>


